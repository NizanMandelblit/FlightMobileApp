<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbarAlwaysDrawHorizontalTrack="false"
    android:scrollbarAlwaysDrawVerticalTrack="true"
    android:scrollbarSize="4dp">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="408dp"
        android:layout_marginBottom="110dp"
        app:layout_constraintBottom_toTopOf="@+id/throttleNameBox"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@android:mipmap/sym_def_app_icon" />

    <TextView
        android:id="@+id/viewUrl"
        android:layout_width="350dp"
        android:layout_height="69dp"
        android:layout_marginBottom="2dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.344"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/rudderNum"
        android:layout_width="26dp"
        android:layout_height="29dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/rudderNameBox"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.954" />

    <TextView
        android:id="@+id/throttleNum"
        android:layout_width="43dp"
        android:layout_height="25dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:layout_marginEnd="50dp"
        android:layout_marginRight="50dp"
        android:ems="10"
        android:inputType="numberDecimal"
        android:rotation="270"
        android:text="0"
        app:layout_constraintEnd_toStartOf="@+id/joystickView"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="497dp" />

    <com.example.flightmobileapp.JoystickView
        android:id="@+id/joystickView"
        android:layout_width="274dp"
        android:layout_height="209dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/elevatorNameBox"
        app:layout_constraintHorizontal_bias="0.85"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aileronNameBox"
        app:layout_constraintVertical_bias="0.164">
        android:on</com.example.flightmobileapp.JoystickView>

    <SeekBar
        android:id="@+id/seekBar2"
        android:layout_width="256dp"
        android:layout_height="34dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/rudderNum"
        app:layout_constraintTop_toBottomOf="@+id/joystickView"
        app:layout_constraintVertical_bias="0.035" />

    <SeekBar
        android:id="@+id/seekBar1"
        android:layout_width="220dp"
        android:layout_height="186dp"
        android:layout_gravity="center"
        android:orientation="vertical"
        android:rotation="270"
        android:scrollbarAlwaysDrawVerticalTrack="true"
        android:tag="Throttle"
        tools:layout_editor_absoluteX="-11dp"
        tools:layout_editor_absoluteY="470dp" />

    <TextView
        android:id="@+id/rudderNameBox"
        android:layout_width="65dp"
        android:layout_height="33dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginBottom="28dp"
        android:text="Rudder"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/throttleNameBox"
        android:layout_width="53dp"
        android:layout_height="57dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:layout_marginBottom="72dp"
        android:orientation="vertical"
        android:rotation="270"
        android:text="Throttle"
        app:layout_constraintBottom_toTopOf="@+id/rudderNameBox"
        app:layout_constraintEnd_toStartOf="@+id/joystickView"
        app:layout_constraintTop_toBottomOf="@+id/throttleNum" />

    <TextView
        android:id="@+id/aileronNameBox"
        android:layout_width="65dp"
        android:layout_height="40dp"
        android:text="Aileron"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <TextView
        android:id="@+id/elevatorNameBox"
        android:layout_width="65dp"
        android:layout_height="104dp"
        android:layout_marginStart="388dp"
        android:layout_marginLeft="388dp"
        android:rotation="270"
        android:text="Elevator"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.844" />

    <TextView
        android:id="@+id/aileronNum"
        android:layout_width="50dp"
        android:layout_height="28dp"
        android:layout_marginBottom="280dp"
        android:text="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.659"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/elevatorNum"
        android:layout_width="28dp"
        android:layout_height="33dp"
        android:layout_marginTop="44dp"
        android:rotation="270"
        android:text="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/elevatorNameBox"
        app:layout_constraintTop_toTopOf="@+id/joystickView" />

</androidx.constraintlayout.widget.ConstraintLayout>